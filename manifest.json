{
    "name":"houdini",
    "description":"send request for page",
    "version":"1.0",
    "manifest_version":3,
    "action":{
        "default_popup":"popup.html",
        "default_icon":{
            "16": "img/wizard_icon.png",
            "48": "img/wizard_icon.png",
            "128": "img/wizard_icon.png"
        }
    },
    "icons": {
        "16": "img/wizard_icon.png",
        "48": "img/wizard_icon.png",
        "128": "img/wizard_icon.png"
    },
    "content_scripts":[
        {
            "matches":["*://*.utp.edu.pe/*"],
            "js":["content-script.js"]
        }
    ]
}